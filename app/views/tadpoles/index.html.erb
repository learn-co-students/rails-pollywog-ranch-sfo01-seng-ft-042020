<h1>Tadpoles</h1>

<% @tadpoles.each do |tadpole| %>
  <div class="card">
    <a href="/tadpoles/<%= tadpole.id %>">
      <img src="http://loremflickr.com/175/115/tadpole?random=<%=tadpole.id%>" alt="Tadpole Picture" height="115" width="175">
    </a>
    <p><a href="/tadpoles/<%= tadpole.id %>"><%= tadpole.name %></a></p>
  </div>
<% end %>